<h1>Log In</h1>

<%= form_tag login_path do %>
  <ul>
    <li>
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
    </li>
    <li>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </li>
    <li>
      <%= submit_tag "Log In" %>
    </li>
  </ul>
<% end %>
